<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"


    app:layout_behavior="@string/appbar_scrolling_view_behavior"

    tools:context=".Main3Activity"
    tools:showIn="@layout/app_bar_main3">


    <SearchView
        android:id="@+id/sv"
        android:layout_width="275dp"
        android:layout_height="61dp"
        android:layout_alignStart="@+id/search_button"
        android:layout_alignEnd="@+id/imageButton4"
        android:layout_marginEnd="47dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="30dp"
        android:layout_height="55dp"
        android:onClick="browse"
        app:srcCompat="@drawable/ic_browse" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="44dp"
        android:layout_height="59dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="5dp"
        android:onClick="onMapSearch"
        app:srcCompat="@android:drawable/ic_search_category_default" />

    <fragment
        android:id="@+id/map1"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_marginTop="68dp"
        tools:context=".MapsActivity" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="190dp"
        android:clickable="true"
        android:onClick="terrane"
        app:srcCompat="@drawable/map1" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/floatingActionButton"
        android:layout_marginBottom="52dp"
        android:clickable="true"
        android:onClick="terrane"
        app:srcCompat="@drawable/map2" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/floatingActionButton2"
        android:layout_marginBottom="55dp"
        android:clickable="true"
        android:onClick="terrane"
        app:srcCompat="@drawable/map3" />


</RelativeLayout>



